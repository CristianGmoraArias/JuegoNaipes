		numerosPosibles.add(agregar);
		Numero agregar= new Numero("4",4);
		numerosPosibles.add(agregar);
		Numero agregar= new Numero("5",5);
		numerosPosibles.add(agregar);
		Numero agregar= new Numero("6",6);
		numerosPosibles.add(agregar);
		Numero agregar= new Numero("7",7);
		numerosPosibles.add(agregar);
		Numero agregar= new Numero("8",8);
		numerosPosibles.add(agregar);
		Numero agregar= new Numero("9",9);
		numerosPosibles.add(agregar);
		Numero agregar= new Numero("10",10);
		numerosPosibles.add(agregar);
		Numero agregar= new Numero("J",10);
		numerosPosibles.add(agregar);
		Numero agregar= new Numero("Q",10);
		numerosPosibles.add(agregar);
		Numero agregar= new Numero("K",110);
		numerosPosibles.add(agregar);
		
		
		
	}
	
	
	
}
